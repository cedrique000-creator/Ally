
cat > .gitignore << 'EOF'
# Dependencies
node_modules/
**/node_modules/

# Environment variables
.env
.env.local
.env.production.local
**/.env

# Build outputs
dist/
build/
**/dist/
**/build/

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
logs/

# OS files
.DS_Store
Thumbs.db
.DS_Store?

# IDE
.vscode/
.idea/
*.swp
*.swo
*.sublime-*

# Vercel
.vercel

# Testing
coverage/
.nyc_output/
EOF